{
    "name": "mycelo",
    "productName": "myCelo",
    "version": "1.4.2",
    "description": "COH2 ranking displayer",
    "license": "MIT",
    "engines": {
        "node": ">=9.0.0",
        "npm": ">=5.0.0",
        "yarn": ">=1.0.0"
    },
    "browserslist": [
        "last 4 versions"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/sepi4/myCeloJs/"
    },
    "publish": {
        "provider": "github",
        "releaseType": "release"
    },
    "main": "main.js",
    "scripts": {
        "make-zip": "npm run make && npm run rm-zip && npm run zip",
        "make": "npm run build && npm run postpackage && npm run cp-img && npm run cp-ranking-files",
        "rm-zip": "@powershell del \"./release-builds/*.zip\"",
        "zip": "@powershell Compress-Archive -Path \"./release-builds/myCelo-win32-x64/*\" -DestinationPath \"./release-builds/myCelo.zip\" -CompressionLevel Optimal",
        "cp-ranking-files": "@powershell Copy-Item \"./rankings.*\" -Destination \"./release-builds/myCelo-win32-x64/\" -PassThru",
        "cp-img": "@powershell Copy-Item \"./img\" \"./release-builds/myCelo-win32-x64\" -Recurse",
        "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
        "start": "cross-env NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
        "build": "cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
        "package": "npm run build",
        "postpackage": "electron-packager ./ --out=./release-builds --overwrite --icon=./img/icon/icon.ico"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.34",
        "@fortawesome/free-solid-svg-icons": "^5.15.2",
        "@fortawesome/react-fontawesome": "^0.1.14",
        "axios": "^0.21.1",
        "cors": "^2.8.5",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-redux": "^7.2.2",
        "redux": "^4.0.5",
        "styled-components": "^5.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.12.13",
        "@babel/preset-env": "^7.12.13",
        "@babel/preset-react": "^7.12.13",
        "babel-loader": "^8.2.2",
        "babili-webpack-plugin": "^0.1.2",
        "cross-env": "^7.0.3",
        "css-loader": "^3.6.0",
        "electron": "^9.4.2",
        "electron-builder": "^22.9.1",
        "electron-devtools-installer": "^3.1.1",
        "electron-packager": "^14.2.1",
        "eslint": "^7.19.0",
        "eslint-plugin-react": "^7.22.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^4.5.1",
        "mini-css-extract-plugin": "^0.9.0",
        "style-loader": "^1.3.0",
        "webpack": "^4.46.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.2"
    }
}
